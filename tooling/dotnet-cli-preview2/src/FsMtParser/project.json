{
    "version": "1.0.0-rc-*",
    "description": "F# Message Templates parser - the ability to parse {named} string values",
    "authors": [ "Adam Chester" ],
    "packOptions": {
        "tags": [ "message", "template", "serilog", "F#", "fsharp", "logging", "semantic", "structured" ],
        "projectUrl": "https://github.com/messagetemplates/messagetemplates-fsharp",
        "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0",
        "iconUrl": "http://messagetemplates.org/images/messagetemplates-nuget.png"
    },
    "buildOptions": {
        "xmlDoc": true,
        "compilerName": "fsc",
        "compile": {
          "includeFiles": [
            "../../../../src/FsMtParser/FsMtParser.fs"
          ]
        }
    },
    "tools": {
      "dotnet-compile-fsc": {
        "version": "1.0.0-preview2-*",
        "imports": [
          "dnxcore50",
          "portable-net45+win81",
          "netstandard1.3"
        ]
      }
    },
    "frameworks": {
        "net45": {
            "dependencies": {
                "FSharp.Core": "4.0.0.1"
            }
        },
        "netstandard1.6": {
            "dependencies": {
                "NETStandard.Library": "1.6.0",
                "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-*"
            }
        }
    }
}